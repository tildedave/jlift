reference_type ::=
    primitive_array_type:a {:  RESULT = a;  :}
 |  reftype_or_expr:a {:  RESULT = a.toType();  :};

class_or_interface_type ::=
    reftype_or_expr:a {:  RESULT = a.toType();  :};

class_type ::=
    class_or_interface_type:a {:  RESULT = a;  :};

name ::=
